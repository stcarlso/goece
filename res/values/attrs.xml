<resources>
	<attr name="group" format="string"/>
	<attr name="affects" format="string"/>
	<declare-styleable name="ColorBand">
		<attr name="pos" format="integer"/>
		<attr name="group"/>
		<attr name="affects"/>
	</declare-styleable>
	<declare-styleable name="ValueEntryBox">
		<attr name="units" format="string"/>
		<attr name="value" format="float"/>
		<attr name="description" format="string"/>
		<attr name="sigfigs" format="integer"/>
		<attr name="allowNegative" format="boolean"/>
		<attr name="group"/>
		<attr name="affects"/>
	</declare-styleable>
	<declare-styleable name="ComplexEntryBox">
		<attr name="units"/>
		<attr name="value"/>
		<attr name="phase" format="float"/>
		<attr name="description"/>
		<attr name="magDesc" format="string"/>
		<attr name="phaDesc" format="string"/>
		<attr name="realDesc" format="string"/>
		<attr name="imagDesc" format="string"/>
		<attr name="sigfigs"/>
		<attr name="group"/>
		<attr name="affects"/>
	</declare-styleable>
	<declare-styleable name="ValueTextBox">
		<attr name="group"/>
		<attr name="affects"/>
	</declare-styleable>
	<declare-styleable name="ResSeriesSpinner">
		<attr name="group"/>
		<attr name="affects"/>
	</declare-styleable>
	<declare-styleable name="CustomEntryBox">
		<attr name="units"/>
		<attr name="description"/>
		<attr name="value"/>
		<attr name="group"/>
		<attr name="affects"/>
		<attr name="sigfigs"/>
		<attr name="customUnits" format="reference"/>
		<attr name="allowNegative"/>
	</declare-styleable>
	<declare-styleable name="ValueOutputField">
		<attr name="description"/>
		<attr name="showDesc" format="boolean"/>
		<attr name="sigfigs"/>
	</declare-styleable>
	<declare-styleable name="PinoutView">
		<attr name="pinMargin" format="dimension"/>
		<attr name="pinLength" format="dimension"/>
		<attr name="pinSpacing" format="dimension"/>
	</declare-styleable>
</resources>